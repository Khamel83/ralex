#!/usr/bin/env python3
"""
Ralex - OpenCode.ai Intelligent Wrapper
Main executable entry point for the Ralex system.
"""

import sys
import os
from pathlib import Path

# Add current directory to Python path
ralex_dir = Path(__file__).parent
sys.path.insert(0, str(ralex_dir))

try:
    from ralex_cli import main
except ImportError:
    print("‚ùå Error: ralex_cli module not found")
    print("Make sure ralex_cli.py is in the same directory as this executable")
    sys.exit(1)

if __name__ == '__main__':
    main()